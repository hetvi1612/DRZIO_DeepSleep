<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="oval">
            <stroke android:width="1.0dip" android:color="@android:color/transparent" />
            <solid android:color="@color/tbtncolor" />
            <size android:height="70.0dip" android:width="70.0dip" />
        </shape>
    </item>
    <item android:state_selected="false">
        <shape android:shape="oval">
            <stroke android:width="1.0dip" android:color="@android:color/transparent" />
            <solid android:color="@color/devidercolor" />
            <size android:height="70.0dip" android:width="70.0dip" />
        </shape>
    </item>
</selector>