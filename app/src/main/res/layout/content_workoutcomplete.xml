<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical"
    android:layoutDirection="ltr"

    app:layout_behavior="@string/appbar_scrolling_view_behavior">


    <include layout="@layout/layout_datashare"/>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@android:color/white"
        android:fillViewport="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:orientation="vertical"
                app:layout_collapseMode="parallax">


                <com.google.android.material.card.MaterialCardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    app:cardCornerRadius="5dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:id="@+id/layone"
                            android:foreground="?attr/selectableItemBackground"

                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="7dp">


                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">


                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal">

                                    <ImageView
                                        android:layout_width="22dp"
                                        android:layout_height="22dp"
                                        android:layout_gravity="center_vertical"
                                        android:src="@drawable/ic_home_exe"
                                        android:tint="@color/tbtncolor" />

                                   <TextView

                                        android:id="@+id/txtexcecount"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="@font/homepagefont"
                                        android:gravity="center"
                                        android:paddingStart="5dp"
                                        android:text="00"
                                        android:textColor="@color/headercolor"
                                        android:textSize="25sp" />

                                </LinearLayout>

                               <TextView

                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:fontFamily="@font/homepagefont"
                                    android:text="@string/exercises"
                                    android:textColor="@color/smalltxtcolor"
                                    android:textSize="16sp" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal">

                                    <ImageView
                                        android:layout_width="22dp"
                                        android:layout_height="22dp"
                                        android:layout_gravity="center_vertical"
                                        android:padding="1dp"
                                        android:src="@drawable/ic_home_cal"
                                        android:tint="@color/tbtncolor" />

                                   <TextView

                                        android:id="@+id/txttotalcal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="@font/homepagefont"
                                        android:gravity="center"
                                        android:paddingStart="5dp"
                                        android:text="0.0"
                                        android:textColor="@color/headercolor"
                                        android:textSize="25sp" />
                                </LinearLayout>


                               <TextView

                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:fontFamily="@font/homepagefont"
                                    android:text="@string/calories"
                                    android:textColor="@color/smalltxtcolor"
                                    android:textSize="16sp" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal">

                                    <ImageView
                                        android:layout_width="22dp"
                                        android:layout_height="22dp"
                                        android:layout_gravity="center_vertical"
                                        android:src="@drawable/ic_home_time"
                                        android:tint="@color/tbtncolor" />

                                   <TextView

                                        android:id="@+id/txtexcertime"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:fontFamily="@font/homepagefont"
                                        android:gravity="center"
                                        android:paddingStart="5dp"
                                        android:text="00:00"
                                        android:textColor="@color/headercolor"
                                        android:textSize="25sp" />
                                </LinearLayout>


                               <TextView

                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:fontFamily="@font/homepagefont"
                                    android:text="@string/duration"
                                    android:textColor="@color/smalltxtcolor"
                                    android:textSize="16sp" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical"
                                android:visibility="gone">

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal">

                                    <ImageView
                                        android:layout_width="22dp"
                                        android:layout_height="22dp"
                                        android:layout_gravity="center_vertical"
                                        android:src="@drawable/ic_navreport"
                                        android:tint="@color/tbtncolor" />

                                   <TextView

                                        android:id="@+id/totaltime"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_horizontal"
                                        android:fontFamily="@font/homepagefont"
                                        android:gravity="center"
                                        android:paddingStart="5dp"
                                        android:text=""
                                        android:textColor="@color/numcolor"
                                        android:textSize="25sp" />
                                </LinearLayout>


                               <TextView

                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:fontFamily="@font/homepagefont"
                                    android:text="@string/report"
                                    android:textColor="@color/smalltxtcolor"
                                    android:textSize="16sp" />
                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="5dp"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:id="@+id/btnconti"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginStart="5dp"
                                android:layout_marginEnd="5dp"
                                android:layout_weight="1"
                                android:background="@drawable/croundrepeat"
                                android:foreground="?attr/selectableItemBackground"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_width="35dp"
                                    android:layout_height="35dp"

                                    android:src="@drawable/ic_home_black_24dp"
                                    android:tint="@color/tbtncolor" />

                               <TextView

                                   android:layout_width="wrap_content"
                                    android:layout_height="50dp"
                                    android:drawableTint="@android:color/white"
                                    android:fontFamily="@font/themefont"
                                    android:gravity="center"
                                    android:text="@string/repeat_day"
                                    android:textColor="@color/tbtncolor"
                                    android:textSize="20sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/btnslayout"

                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginStart="5dp"
                                android:layout_marginEnd="5dp"
                                android:layout_weight="1"
                                android:background="@drawable/croundshare"
                                android:foreground="?attr/selectableItemBackground"
                                android:gravity="center"
                                android:orientation="horizontal">


                               <TextView

                                   android:layout_width="wrap_content"
                                    android:layout_height="50dp"
                                    android:layout_marginEnd="5dp"
                                    android:drawableTint="@android:color/white"
                                    android:fontFamily="@font/themefont"
                                    android:gravity="center"
                                    android:text="@string/share"
                                    android:textColor="@android:color/white"
                                    android:textSize="20sp"
                                    android:textStyle="bold" />

                                <ImageView
                                    android:layout_width="35dp"
                                    android:layout_height="35dp"
                                    android:padding="5dp"
                                    android:src="@drawable/ic_share_black_24dp"
                                    android:tint="@android:color/white" />


                            </LinearLayout>

                        </LinearLayout>
                    </LinearLayout>
                </com.google.android.material.card.MaterialCardView>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <ImageView
                        android:id="@+id/btnfb"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_weight="1"
                        android:padding="10dp"
                        android:src="@drawable/ic_myfacebook"
                        android:tint="@color/themegreen" />

                    <ImageView
                        android:id="@+id/btnwhats"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_weight="1"
                        android:padding="10dp"
                        android:src="@drawable/ic_mywhatsapp"
                        android:tint="@color/themegreen" />

                    <ImageView
                        android:id="@+id/btntwite"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_weight="1"
                        android:padding="10dp"
                        android:src="@drawable/ic_mytwitter"
                        android:tint="@color/themegreen" />

                    <ImageView
                        android:id="@+id/btnshareall"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:layout_weight="1"
                        android:padding="10dp"
                        android:src="@drawable/ic_myshare"
                        android:tint="@color/themegreen" />
                </LinearLayout>


            </LinearLayout>


            <com.google.android.material.card.MaterialCardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                app:cardCornerRadius="5dp">

                <RelativeLayout
                    android:id="@+id/btreminder"
                    android:layout_width="match_parent"

                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:foreground="?attr/selectableItemBackground"
                    android:orientation="vertical">


                   <TextView

                        android:id="@+id/tgg"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/themefont"
                        android:text="@string/reminder"

                        android:textColor="@color/headercolor"
                        android:textSize="20sp"
                        android:textStyle="bold" />


                   <TextView

                        android:id="@+id/txttime"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tgg"
                        android:fontFamily="@font/themefont"
                        android:text="20:00"
                        android:textColor="@color/tbtncolor"
                        android:textSize="18sp"
                        android:visibility="gone" />


                    <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:layout_alignParentEnd="true"
                        android:padding="10dp"
                        android:src="@drawable/ic_remindericon"
                        android:tint="@color/tbtncolor" />


                </RelativeLayout>

            </com.google.android.material.card.MaterialCardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                app:cardCornerRadius="5dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:gravity="center_vertical"
                    android:orientation="vertical">


                    <LinearLayout
                        android:id="@+id/btcalbmi"
                        android:layout_width="match_parent"

                        android:layout_height="wrap_content"
                        android:layout_margin="10dp"
                        android:foreground="?attr/selectableItemBackground"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:gravity="center_vertical">

                           <TextView

                                android:id="@+id/cal"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/themefont"
                                android:text="@string/calculate_bmi"

                                android:textColor="@color/headercolor"
                                android:textSize="20sp"
                                android:textStyle="bold" />

                           <TextView

                                android:id="@+id/txtbmi"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/cal"
                                android:fontFamily="@font/themefont"
                                android:text="0.0 KG/M²"
                                android:textColor="@color/tbtncolor"
                                android:textSize="18sp" />

                            <ImageView
                                android:layout_width="40dp"
                                android:layout_height="40dp"
                                android:layout_alignParentEnd="true"
                                android:src="@drawable/img_icedit"
                                android:tint="@color/tbtncolor" />
                        </RelativeLayout>


                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginEnd="10dp"
                        android:background="@color/devidercolor" />

                    <RelativeLayout
                        android:id="@+id/bmiscalelay"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:scrollbars="vertical"
                        android:visibility="gone">

                        <RelativeLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <RelativeLayout
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <RelativeLayout
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="10.0dip">

                                   <TextView

                                        android:id="@+id/wght1"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="10.0dip"
                                        android:fontFamily="@font/themefont"
                                        android:text="@string/weight"
                                        android:textColor="@color/headercolor"
                                        android:textSize="15.0sp"
                                        android:textStyle="bold" />

                                   <TextView

                                        android:id="@+id/wght2"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_margin="10.0dip"
                                        android:layout_marginStart="10.0dip"
                                        android:layout_toEndOf="@+id/wght1"
                                        android:fontFamily="@font/themefont"
                                        android:text=""
                                        android:textColor="@color/tbtncolor"
                                        android:textSize="15.0sp"
                                        android:textStyle="normal" />

                                    <drzio.insomnia.treatment.bedtime.yoga.sleep.customs.CustomSeekBar
                                        android:id="@+id/bmiseekBar"
                                        android:layout_width="fill_parent"
                                        android:layout_height="25.0dip"
                                        android:layout_below="@+id/wght2"
                                        android:layout_marginTop="10.0dip"
                                        android:max="50"
                                        android:progress="0"
                                        android:progressDrawable="@android:color/transparent"
                                        android:thumb="@drawable/seek_thumb_normal"
                                        android:thumbOffset="12.0dip"
                                        android:thumbTint="@color/headercolor" />

                                   <TextView

                                        android:id="@+id/wght3"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@+id/bmiseekBar"
                                        android:layout_marginTop="10.0dip"
                                        android:fontFamily="@font/themefont"
                                        android:text="@string/bmi_kg_m"
                                        android:textAllCaps="false"
                                        android:textColor="@color/headercolor"
                                        android:textSize="15.0sp"
                                        android:textStyle="bold" />

                                   <TextView

                                        android:id="@+id/wght4"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@+id/bmiseekBar"
                                        android:layout_marginStart="10.0dip"
                                        android:layout_marginTop="10.0dip"
                                        android:layout_toEndOf="@+id/wght3"
                                        android:fontFamily="@font/themefont"
                                        android:text=""
                                        android:textColor="@color/tbtncolor"
                                        android:textSize="15.0sp"
                                        android:textStyle="normal" />

                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@+id/wght3"
                                        android:layout_marginTop="10.0dip"
                                        android:orientation="vertical">

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:orientation="horizontal">

                                            <ImageView
                                                android:layout_width="10.0dip"
                                                android:layout_height="10.0dip"
                                                android:layout_gravity="center"
                                                android:background="@color/violet" />

                                           <TextView

                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center"
                                                android:layout_marginStart="10.0dip"
                                                android:fontFamily="@font/themefont"
                                                android:text="0-15"
                                                android:textColor="@color/smalltxtcolor"
                                                android:textSize="15.0sp"
                                                android:textStyle="normal" />
                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:orientation="horizontal">

                                            <ImageView
                                                android:layout_width="10.0dip"
                                                android:layout_height="10.0dip"
                                                android:layout_gravity="center"
                                                android:background="@color/blue" />

                                           <TextView

                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center"
                                                android:layout_marginStart="10.0dip"
                                                android:fontFamily="@font/themefont"
                                                android:text="16-18"
                                                android:textColor="@color/smalltxtcolor"
                                                android:textSize="15.0sp"
                                                android:textStyle="normal" />
                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:orientation="horizontal">

                                            <ImageView
                                                android:layout_width="10.0dip"
                                                android:layout_height="10.0dip"
                                                android:layout_gravity="center"
                                                android:background="@color/green" />

                                           <TextView

                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center"
                                                android:layout_marginStart="10.0dip"
                                                android:fontFamily="@font/themefont"
                                                android:text="19-25"
                                                android:textColor="@color/smalltxtcolor"
                                                android:textSize="15.0sp"
                                                android:textStyle="normal" />
                                        </LinearLayout>
                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_below="@+id/wght3"
                                        android:layout_marginStart="150.0dip"
                                        android:layout_marginTop="10.0dip"
                                        android:orientation="vertical">

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:orientation="horizontal">

                                            <ImageView
                                                android:layout_width="10.0dip"
                                                android:layout_height="10.0dip"
                                                android:layout_gravity="center"
                                                android:background="@color/yellow" />

                                           <TextView

                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center"
                                                android:layout_marginStart="10.0dip"
                                                android:fontFamily="@font/themefont"
                                                android:text="26-30"
                                                android:textColor="@color/smalltxtcolor"
                                                android:textSize="15.0sp"
                                                android:textStyle="normal" />
                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:orientation="horizontal">

                                            <ImageView
                                                android:layout_width="10.0dip"
                                                android:layout_height="10.0dip"
                                                android:layout_gravity="center"
                                                android:background="@color/orange" />

                                           <TextView

                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center"
                                                android:layout_marginStart="10.0dip"
                                                android:fontFamily="@font/themefont"
                                                android:text="31-40"
                                                android:textColor="@color/smalltxtcolor"
                                                android:textSize="15.0sp"
                                                android:textStyle="normal" />
                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:orientation="horizontal">

                                            <ImageView
                                                android:layout_width="10.0dip"
                                                android:layout_height="10.0dip"
                                                android:layout_gravity="center"
                                                android:background="@color/red" />

                                           <TextView

                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_gravity="center"
                                                android:layout_marginStart="10.0dip"
                                                android:fontFamily="@font/themefont"
                                                android:text="41-50"
                                                android:textColor="@color/smalltxtcolor"
                                                android:textSize="15.0sp"
                                                android:textStyle="normal" />
                                        </LinearLayout>
                                    </LinearLayout>
                                </RelativeLayout>
                            </RelativeLayout>
                        </RelativeLayout>

                    </RelativeLayout>


                </LinearLayout>

            </androidx.cardview.widget.CardView>


            <androidx.cardview.widget.CardView
                android:id="@+id/blogdetail"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:visibility="gone"
                app:cardBackgroundColor="@color/colorPrimaryDark"
                app:cardCornerRadius="8dp"
                app:cardElevation="8dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imgthumb"
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:scaleType="centerCrop" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/imgthumb"
                        android:layout_marginStart="20dp"
                        android:orientation="vertical">

                       <TextView

                            android:id="@+id/name"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:layout_marginEnd="40dp"
                            android:fontFamily="@font/themefont"
                            android:singleLine="true"
                            android:text="potato"
                            android:textColor="@android:color/white"
                            android:textSize="18sp"
                            android:textStyle="bold" />

                        <org.sufficientlysecure.htmltextview.HtmlTextView
                            android:id="@+id/descriptions"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:layout_marginEnd="40dp"
                            android:layout_marginBottom="5dp"
                            android:fontFamily="@font/themefont"
                            android:maxLines="1"
                            android:text="potato"
                            android:textColor="@android:color/white"
                            android:textSize="14sp" />

                    </LinearLayout>


                </RelativeLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cardadd"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:visibility="gone"
                app:cardBackgroundColor="@color/colorPrimaryDark"
                app:cardCornerRadius="5dp"
                app:cardElevation="8dp">

                <LinearLayout
                    android:id="@+id/adframe2"
                    android:layout_width="match_parent"
